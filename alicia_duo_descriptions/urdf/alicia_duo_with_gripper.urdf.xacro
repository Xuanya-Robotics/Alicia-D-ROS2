<?xml version="1.0"?>
<robot name="alicia_duo_descriptions" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- =================================================================== -->
  <!-- | XACRO PROPERTIES                                                | -->
  <!-- =================================================================== -->
  <xacro:property name="PI" value="3.1415926535897931" />
  <xacro:property name="mesh_prefix" value="package://alicia_duo_descriptions/meshes/" />

  <!-- =================================================================== -->
  <!-- | XACRO MACROS                                                    | -->
  <!-- =================================================================== -->

  <!-- Macro for visual and collision properties of a link -->
  <xacro:macro name="visual_collision_geometry" params="link_name">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}${link_name}.STL" />
      </geometry>
      <material name="">
        <color rgba="1 1 1 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="${mesh_prefix}${link_name}.STL" />
      </geometry>
    </collision>
  </xacro:macro>

  <!-- Macro for a revolute joint -->
  <xacro:macro name="revolute_joint" params="name parent child origin_xyz origin_rpy axis_xyz lower_limit upper_limit">
    <joint name="${name}" type="revolute">
      <origin xyz="${origin_xyz}" rpy="${origin_rpy}" />
      <parent link="${parent}" />
      <child link="${child}" />
      <axis xyz="${axis_xyz}" />
      <limit lower="${lower_limit}" upper="${upper_limit}" effort="12" velocity="5" />
    </joint>
  </xacro:macro>

  <!-- Macro for a fixed joint -->
  <xacro:macro name="fixed_joint" params="name parent child origin_xyz origin_rpy">
    <joint name="${name}" type="fixed">
      <origin xyz="${origin_xyz}" rpy="${origin_rpy}" />
      <parent link="${parent}" />
      <child link="${child}" />
    </joint>
  </xacro:macro>

  <!-- =================================================================== -->
  <!-- | ROBOT BODY                                                      | -->
  <!-- =================================================================== -->

  <!-- Base Link -->
  <link name="base_link">
    <xacro:visual_collision_geometry link_name="base_link" />
  </link>

